---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Questions</title>
	<link href="http://isaacmc.github.io/blog/atom.xml" rel="self"/>
	<link href="http://isaacmc.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://isaacmc.github.io/blog</id>
	<author>
		<name>Isaac Craig</name>
		<email>isaacmc@gmail.com</email>
	</author>

	{% for question in site.questions %}
		<entry>
			<title>{{ question.title }}</title>
			<link href="http://isaacmc.github.io{{ question.url }}"/>
			<updated>{{ question.date | date_to_xmlschema }}</updated>
			<id>http://isaacmc.github.io/{{ question.id }}</id>
			<content type="html">{{ question.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
